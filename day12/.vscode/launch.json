{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "Profiler",
      "type": "python",
      "request": "launch",
      "module": "cProfile",
      "args": ["-o", "${workspaceFolder}\\tmp.txt", "${file}"]
    }
  ]
}
